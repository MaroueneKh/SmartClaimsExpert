    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.marouenekhadhraoui.smartclaimsexpert.ui.detailsDossier.PlanifierVisioViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
