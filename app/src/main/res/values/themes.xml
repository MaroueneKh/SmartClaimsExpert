<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SmartClaimsExpert" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/mainblue</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/white</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorOnSurface">@color/black</item>
        <item name="android:colorBackground">@color/white</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="bottomNavigationStyle">@style/Widget.App.BottomNavigationView</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
    </style>

    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorPrimary">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
    </style>

    <style name="ThemeOverlay.App.floatingButton" parent="">
        <item name="colorOnSecondary">@color/mainblue</item>
        <item name="colorOnSurface">@color/mainblue</item>
    </style>

    <style name="Theme.SmartClaimsExpert.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="SmartClaimsText">
        <item name="android:textColor">@color/mainblue</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:text">@string/SmartsClaims</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">true</item>

    </style>
    <style name="DashboardText">
        <item name="android:textColor">@color/mainblue</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:text">@string/DashboardExpert</item>
        <item name="android:maxLines">2</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="BienvenueText">
        <item name="android:textColor">@color/greylight</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:maxLines">2</item>
        <item name="android:text">@string/Bienvenue</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="AssureNomText">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="NameTextinButtom">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="EnligneText">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:maxLines">1</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:text">@string/Enligne</item>

    </style>
    <style name="DetailsText">
        <item name="android:textColor">@color/mainblue</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:maxLines">2</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:text">@string/Enligne</item>

    </style>
    <style name="RadioButtonGroup" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">18sp</item>
        <item name="colorControlNormal">@color/mainblue</item>
        <item name="android:buttonTint">@color/mainblue</item>
    </style>



    <style name="Theme.SmartClaims.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SmartClaims.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

=




</resources>